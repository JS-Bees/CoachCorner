{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "lerna": "^7.2.0"
  },
  "scripts": {
    "start:app": "echo 'Starting backend' && cd ./backend && npm start && echo 'Starting frontend' && cd ./frontend && npm start",
    "lintFrontend": "cd frontend && npm run lint",
    "lintBackend": "cd backend && npm run lint",
    "installDepBackend": "npm --prefix ./backend run installDep",
    "installDepFrontend": "npm --prefix ./frontend run installDep"
  },
  "dependencies": {
    "@mui/icons-material": "^5.14.16",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "moment": "^2.29.4",
    "react-native-elements": "^3.4.3",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.10.6",
    "react-native-paper-dates": "^0.19.7",
    "react-native-screens": "^3.27.0",
    "react-native-text": "^1.2.2",
    "react-native-vector-icons": "^10.0.0"
  }
}
